{
  "name": "@modstrap/tooltip",
  "version": "1.0.0",
  "description": "Adaptation for Tippy.js.",
  "main": "dist/Tooltip.js",
  "types": "dist/Tooltip.d.ts",
  "style": "dist/Tooltip.css",
  "author": "Callisto (https://github.com/callisto2410)",
  "license": "MIT",
  "homepage": "https://github.com/callisto2410/modstrap-tooltip",
  "bugs": {
    "url": "https://github.com/callisto2410/modstrap-tooltip/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callisto2410/modstrap-tooltip.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json && npx sass --no-source-map src/Tooltip.scss dist/Tooltip.css",
    "build:all": "npm run build:clean && npm run build && npm run build:demo",
    "build:demo": "webpack --config webpack.config.js",
    "build:demo:watch": "webpack --watch --config webpack.config.js",
    "build:clean": "npx rimraf .cache dist demo/dist *.tgz",
    "build:pack": "npm run build:clean && npm run build && npm pack"
  },
  "devDependencies": {
    "@types/node": "^14.10.1",
    "autoprefixer": "^10.0.4",
    "css-loader": "^5.0.1",
    "postcss-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@modstrap/lexicon": "git+https://github.com/callisto2410/modstrap-lexicon.git#v1.0.0",
    "tippy.js": "^6.2.7"
  }
}
